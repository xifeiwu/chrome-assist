{
  "name": "assist",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/content.js"]
    }
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_title": "assist chrome-extension",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "storage",
    "http://*/",
    "https://*/"
  ]
}